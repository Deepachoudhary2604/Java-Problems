public class Main {
  public static void main(String[] args) {
    int[] arr={1,2,3,4,5,6,7,8,9,4};
    boolean check=sorted(arr);
    System.out.println(check);
  }
  public static boolean sorted(int[] arr){
    for(int i=0;i<arr.length;i++){
      if(arr[i]>arr[i+1]){
        return false;
      }
      
    }
    return true;
  }
  

}
